{
  "expo": {
    "name": "FitQii",
    "slug": "FitQii",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "fitqii",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./src/assets/fonts/SpaceGrotesk-Light.ttf",
            "./src/assets/fonts/SpaceGrotesk-Regular.ttf",
            "./src/assets/fonts/SpaceGrotesk-Medium.ttf",
            "./src/assets/fonts/SpaceGrotesk-SemiBold.ttf",
            "./src/assets/fonts/SpaceGrotesk-Bold.ttf"
          ],
          "android": {
            "fonts": [
              {
                "fontFamily": "Space Grotesk",
                "fontDefinitions": [
                  {
                    "path": "./src/assets/fonts/SpaceGrotesk-Light.ttf",
                    "weight": 300
                  },
                  {
                    "path": "./src/assets/fonts/SpaceGrotesk-Regular.ttf",
                    "weight": 400
                  },
                  {
                    "path": "./src/assets/fonts/SpaceGrotesk-Medium.ttf",
                    "weight": 500
                  },
                  {
                    "path": "./src/assets/fonts/SpaceGrotesk-SemiBold.ttf",
                    "weight": 600
                  },
                  {
                    "path": "./assets/fonts/Inter-Bold.ttf",
                    "weight": 700
                  }
                ]
              }
            ]
          },
          "ios": {
            "fonts": [
              {
                "fontFamily": "Space Grotesk",
                "fontDefinitions": [
                  {
                    "path": "./src/assets/fonts/SpaceGrotesk-Light.ttf",
                    "weight": 300
                  },
                  {
                    "path": "./src/assets/fonts/SpaceGrotesk-Regular.ttf",
                    "weight": 400
                  },
                  {
                    "path": "./src/assets/fonts/SpaceGrotesk-Medium.ttf",
                    "weight": 500
                  },
                  {
                    "path": "./src/assets/fonts/SpaceGrotesk-SemiBold.ttf",
                    "weight": 600
                  },
                  {
                    "path": "./assets/fonts/Inter-Bold.ttf",
                    "weight": 700
                  }
                ]
              }
            ]
          }
        }
      ],
      [
        "expo-sqlite",
        {
          "enableFTS": true,
          "useSQLCipher": true,
          "android": {
            "enableFTS": false,
            "useSQLCipher": false
          },
          "ios": {
            "customBuildFlags": [
              "-DSQLITE_ENABLE_DBSTAT_VTAB=1 -DSQLITE_ENABLE_SNAPSHOT=1"
            ]
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
